import(stats, Rcmdr)

importFrom(tcltk,
    setTkProgressBar, tcl, tclRequire, "tclvalue<-", 
	tclVar, tkbind, tkcheckbutton, tkconfigure, tkdelete, 
	tkdestroy, tkfont.create, tkframe, tkgrid, tkinsert, 
	tklabel, tkpack, tkradiobutton, tkscale, tkselection.clear, 
	tktoplevel, tkwidget, tkwm.geometry, tkwm.title, ttkprogressbar)

exportPattern("^[^\\.]")
S3method(anova, coxph)
S3method(plot, coxph)
S3method(quantile, survfit)
S3method(unfold, data.frame)

